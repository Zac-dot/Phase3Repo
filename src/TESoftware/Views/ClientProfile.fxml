<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TESoftware.Controllers.ClientProfile">
   <children>
      <Label fx:id="nameLabel" alignment="TOP_CENTER" layoutX="45.0" layoutY="38.0" prefHeight="27.0" prefWidth="210.0" text="Welcome, Philip" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label alignment="TOP_CENTER" layoutX="14.0" layoutY="118.0" prefHeight="18.0" prefWidth="144.0" text="Check your order history" />
      <Label alignment="TOP_CENTER" layoutX="14.0" layoutY="171.0" prefHeight="18.0" prefWidth="144.0" text="Make a new order" />
      <Button fx:id="orderID" layoutX="197.0" layoutY="114.0" mnemonicParsing="false" onAction="#Order" text="Order History" />
      <Button fx:id="OrderButtonID" layoutX="213.0" layoutY="168.0" mnemonicParsing="false" onAction="#NewOrder" text="Order..." />
      <Button fx:id="closeButton" layoutX="19.0" layoutY="260.0" mnemonicParsing="false" onAction="#Logout" text="Logout" />
   </children>
</AnchorPane>
